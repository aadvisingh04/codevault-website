{
  "semi": true,
  "doubleQuotes": false,
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 80,
  "jsxDoubleQuote": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "bracketSpacing": true,
  "trailingComma": "all",
  "overrides": [
    {
      "files": "*.css",
      "options": {
        "singleQuote": false
      }
    },
    {
      "files": "*.jsx",
      "options": {
        "singleQuote": false
      }
    }
  ]
}
